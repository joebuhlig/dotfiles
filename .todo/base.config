export TODO_DIR="$HOME/Documents/todo"
export TODO_ACTIONS_DIR="$HOME/.todo/actions"
export REPORT_FILE="$TODO_DIR/report.txt"

export PRI_A=$YELLOW
export PRI_B=$LIGHT_RED
export PRI_C=$LIGHT_GREEN
export PRI_D=$BROWN
export PRI_X=$WHITE

export COLOR_DONE=$LIGHT_GREY

export COLOR_PROJECT=$LIGHT_PURPLE
export COLOR_CONTEXT=$LIGHT_BLUE

export TODOTXT_FINAL_FILTER="$HOME/.todo/actions/futureTasks"
export TODOTXT_SORT_COMMAND='env LC_COLLATE=C sort -k 2,2 -k 1,1n'
